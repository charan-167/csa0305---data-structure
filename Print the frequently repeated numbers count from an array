#include <stdio.h>

int main() {
    int n;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int arr[n], count[n];
    for(int i=0;i<n;i++) {
        scanf("%d", &arr[i]);
        count[i]=-1;
    }

    for(int i=0;i<n;i++) {
        int c=1;
        for(int j=i+1;j<n;j++) {
            if(arr[i]==arr[j]) {
                c++;
                count[j]=0;
            }
        }
        if(count[i]!=0) count[i]=c;
    }

    printf("Frequency of elements:\n");
    for(int i=0;i<n;i++) {
        if(count[i]!=0)
            printf("%d occurs %d times\n", arr[i], count[i]);
    }
    return 0;
}
