#include <stdio.h>

int main() {
    int arr[8] = {1, 2, 3, 2, 4, 5, 1, 6};
    int i, j;

    printf("Duplicate elements: ");
    for (i = 0; i < 8; i++) {
        for (j = i + 1; j < 8; j++) {
            if (arr[i] == arr[j]) {
                printf("%d ", arr[i]);
                break;
            }
        }
    }
    return 0;
}
