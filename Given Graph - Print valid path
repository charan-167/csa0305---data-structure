#include <stdio.h>

int visited[20], graph[20][20], n;

void dfs(int start, int end) {
    visited[start] = 1;
    printf("%d ", start);
    if(start == end) return;
    for(int i=0; i<n; i++) {
        if(graph[start][i] && !visited[i]) {
            dfs(i,end);
        }
    }
}

int main() {
    int start, end;
    printf("Enter number of vertices: ");
    scanf("%d", &n);

    printf("Enter adjacency matrix:\n");
    for(int i=0; i<n; i++)
        for(int j=0; j<n; j++)
            scanf("%d", &graph[i][j]);

    printf("Enter start and end node: ");
    scanf("%d %d", &start, &end);

    printf("Path: ");
    dfs(start,end);
    printf("\n");
    return 0;
}
