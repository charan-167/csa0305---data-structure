#include <stdio.h>

int main() {
    int arr[100], n;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Enter elements: ");
    for(int i=0; i<n; i++) scanf("%d", &arr[i]);

    if(n >= 5)
        printf("5th element is: %d\n", arr[4]);
    else
        printf("Array has less than 5 elements.\n");

    return 0;
}
