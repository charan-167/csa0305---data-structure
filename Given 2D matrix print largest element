#include <stdio.h>

int main() {
    int r, c;
    printf("Enter rows and cols: ");
    scanf("%d %d", &r, &c);

    int mat[r][c];
    printf("Enter elements:\n");
    for(int i=0;i<r;i++)
        for(int j=0;j<c;j++)
            scanf("%d", &mat[i][j]);

    int max=mat[0][0];
    for(int i=0;i<r;i++)
        for(int j=0;j<c;j++)
            if(mat[i][j]>max) max=mat[i][j];

    printf("Largest element: %d\n", max);
    return 0;
}
