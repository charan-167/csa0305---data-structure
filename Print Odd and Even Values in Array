#include <stdio.h>

int main() {
    int arr[6] = {10, 15, 20, 25, 30, 35};
    int i;

    printf("Even numbers: ");
    for (i = 0; i < 6; i++) if (arr[i] % 2 == 0) printf("%d ", arr[i]);

    printf("\nOdd numbers: ");
    for (i = 0; i < 6; i++) if (arr[i] % 2 != 0) printf("%d ", arr[i]);

    return 0;
}
