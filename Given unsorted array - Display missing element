#include <stdio.h>

int main() {
    int n, arr[100], sum=0, total;
    printf("Enter n (array size): ");
    scanf("%d", &n);

    printf("Enter %d elements (1 to n+1 with one missing): ", n);
    for(int i=0; i<n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    total = (n+1) * (n+2) / 2; // sum of first (n+1) natural numbers
    printf("Missing element: %d\n", total - sum);
    return 0;
}
