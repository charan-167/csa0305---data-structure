#include <stdio.h>

int main() {
    int arr[6] = {10, 20, 30, 40, 50, 60};
    int key, i, found = 0;

    printf("Enter element to search: ");
    scanf("%d", &key);

    for (i = 0; i < 6; i++) {
        if (arr[i] == key) {
            printf("Element %d found at position %d\n", key, i + 1);
            found = 1;
            break;
        }
    }
    if (!found) printf("Element not found\n");

    return 0;
}
