#include <stdio.h>

int main() {
    int n, i, j, edges=0;
    printf("Enter number of vertices: ");
    scanf("%d", &n);

    int graph[n][n];
    printf("Enter adjacency matrix:\n");
    for(i=0; i<n; i++) {
        for(j=0; j<n; j++) {
            scanf("%d", &graph[i][j]);
        }
    }

    for(i=0; i<n; i++) {
        for(j=i+1; j<n; j++) {
            if(graph[i][j] == 1) edges++;
        }
    }

    printf("Minimum number of edges: %d\n", edges);
    return 0;
}
