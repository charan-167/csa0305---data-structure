#include <stdio.h>
#include <ctype.h>

int main() {
    char str[100];
    int i, valid = 1;

    printf("Enter a string: ");
    gets(str);

    for (i = 0; str[i] != '\0'; i++) {
        if (!isalpha(str[i]) && str[i] != ' ') {
            valid = 0;
            break;
        }
    }

    if (valid)
        printf("Valid String\n");
    else
        printf("Invalid String\n");

    return 0;
}
